{
    "version": "2.0.0",
    "command": "g++",
    "args": ["-g","./BehavioralModel/Strategy/main.cpp","-o","${fileBasenameNoExtension}.exe", "-I", "./BehavioralModel/Iterator/"], // 编译命令参数
    "problemMatcher": {
        "owner": "cpp",
        "fileLocation": ["relative", "${workspaceRoot}"],
        "pattern": {
            "regexp": "^(.*):(\\d+):(\\d+):\\s+(warning|error):\\s+(.*$",
            "file": 1,
            "line": 2,
            "column": 3,
            "severity": 4,
            "message": 5
        }
    }
  }
  